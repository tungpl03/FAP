<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.login</servlet-class>
        <init-param>
            <param-name>errlogin</param-name>
            <param-value>*Account does not exist</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>tofeedbackpage</servlet-name>
        <servlet-class>Controller.tofeedbackpage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tofbpage</servlet-name>
        <servlet-class>Controller.tofbpage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tofeedbackpageteacher</servlet-name>
        <servlet-class>Controller.tofeedbackpageteacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tofbpageforteacher</servlet-name>
        <servlet-class>Controller.tofbpageforteacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tocommentedit</servlet-name>
        <servlet-class>Controller.tocommentedit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tofeedbackpage</servlet-name>
        <url-pattern>/tofeedbackpage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tofbpage</servlet-name>
        <url-pattern>/tofbpage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tofeedbackpageteacher</servlet-name>
        <url-pattern>/tofeedbackpageteacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tofbpageforteacher</servlet-name>
        <url-pattern>/tofbpageforteacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tocommentedit</servlet-name>
        <url-pattern>/tocommentedit</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
